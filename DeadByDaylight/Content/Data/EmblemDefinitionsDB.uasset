DefaultDescription: Benevolent Starting Value
DescriptionsByQuality: [  ]
 }, { 
EmblemProgressionType: EEmblemProgressionType::SurvivorBenevolentHealing
DefaultDescription: Survivors Healed
DescriptionsByQuality: [  ]
 }, { 
EmblemProgressionType: EEmblemProgressionType::SurvivorBenevolentHooked
DefaultDescription: Hooked Survivors
DescriptionsByQuality: [  ]
 }, { 
EmblemProgressionType: EEmblemProgressionType::SurvivorBenevolentUnhook
DefaultDescription: Unhooked Survivors
DescriptionsByQuality: [  ]
 }, { 
EmblemProgressionType: EEmblemProgressionType::SurvivorBenevolentPerformUnhook
DefaultDescription: Safe Hook Rescues
DescriptionsByQuality: [  ]
 }, { 
EmblemProgressionType: EEmblemProgressionType::SurvivorBenevolentSaveFromKiller
DefaultDescription: Saves From Killer's Grasp
DescriptionsByQuality: [  ]
 }, { 
